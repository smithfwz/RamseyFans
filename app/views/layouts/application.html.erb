<!DOCTYPE html>
<html>
<head>
  <title>Ramseyfans</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

	<div class="search">
		<%= form_tag('/dishes', :method => 'get', id: "search-form") do %>
			<%= text_field_tag :search, @keyword, placeholder: "search" %>
			<%= submit_tag "Search", :name => nil %>
		<% end %>
	</div>

<%= yield %>

</body>
</html>
